{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2019",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
			"@/*": ["src/*"]
    },
    "strict": true,
    "module": "es2015",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "importHelpers": true
  }
}